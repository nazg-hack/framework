{
  "name": "nazg/framework",
  "description": "Http Microframework for Hack",
  "keywords": [
    "hhvm",
    "hack",
    "cache",
    "middleware",
    "dependency injection"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Yuuki Takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "require": {
    "hhvm": "^4.35",
    "hhvm/hsl": "^4.0",
    "hhvm/hsl-experimental": "^4.25",
    "hhvm/hhvm-autoload": "^2.0",
    "facebook/hh-clilib": "^2.2.4",
    "hack-psr/psr7-http-message-hhi": "^1.0.0",
    "ytake/hungrr": "^0.8",
    "ytake/hhypermedia": "^0.3.2",
    "nazg/glue": "^1.3",
    "nazg/heredity": "^1.7",
    "nazg/hcache": "^0.4",
    "nazg/http-server-request-handler": "^0.3.0",
    "nazg/http-executor": "^0.7",
    "facebook/hack-router": ">=0.17 <1.0",
    "facebook/hack-http-request-response-interfaces": "^0.2",
    "hack-logging/hack-logging": "^0.3"
  },
  "require-dev": {
    "hhvm/hacktest": "^2.0.0",
    "facebook/fbexpect": "^2.7.3",
    "hhvm/hhast": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "Nazg\\": "src/"
    },
    "files": [
      "src/http_enum.php",
      "src/types.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "NazgTest\\": "tests/"
    },
    "files": [
      "tests/Struct/Shapes.php"
    ]
  },
  "minimum-stability": "stable",
  "scripts": {
    "tests": [
      "hhvm ./vendor/bin/hacktest.hack tests/"
    ],
    "register": [
      "hhvm ./vendor/bin/hh-autoload.hack"
    ]
  }
}
